{
  "name": "datepickertest",
  "version": "0.0.1",
  "main": "./dist/datepickertest.umd.js",
  "module": "./dist/datepickertest.es.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/datepickertest.es.js",
      "require": "./dist/datepickertest.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:library": "tsx buildLibrary.ts",
    "vue:tsx": "vue-tsc -p tsconfig.lib.json --declaration --emitDeclarationOnly",
    "build-t": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "build-test": "vite build && vue-tsc",
    "preview": "vite preview",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "vitest:ui": "vitest --ui",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:fix": "eslint --fix --ext .js,.vue src",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.12",
    "@typescript-eslint/typescript-estree": "^7.3.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.5.0",
    "@vitest/ui": "^1.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.0.3",
    "sass": "^1.72.0",
    "stylelint": "^16.2.1",
    "tsx": "^4.11.0",
    "typescript": "~5.4.0",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-eslint2": "^4.4.0",
    "vite-plugin-mkcert": "^1.17.5",
    "vitest": "^1.5.0",
    "vue-tsc": "^2.0.17"
  },
  "resolutions": {
    "@typescript-eslint/typescript-estree": "^5.59.2"
  }
}
